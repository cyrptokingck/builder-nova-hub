{
  "expo": {
    "name": "LottoCoin",
    "slug": "lottocoin-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#CAF206"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.lottocoin.mobile",
      "infoPlist": {
        "UIBackgroundModes": ["background-fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#CAF206"
      },
      "package": "com.lottocoin.mobile",
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
